<?xml version="1.0" encoding="utf-8"?>
<!-- ic_broadcast.xml
     Concentric WiFi/signal arc waves â€” represents Bluetooth/WiFi broadcasting.
     Used in the volunteer banner and on the Save button in activity_volunteer.xml.
     Tinted #FF4F00 in the layout. -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- Center dot -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M12,19 A1.5,1.5 0 1 1 12,22 A1.5,1.5 0 1 1 12,19z" />

    <!-- Inner arc -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M8.5,15.5 A5,5 0 0 1 15.5,15.5" />

    <!-- Middle arc -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M5,12 A8.5,8.5 0 0 1 19,12" />

    <!-- Outer arc -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:pathData="M1.5,8.5 A12,12 0 0 1 22.5,8.5" />

</vector>